func void fib(int n):
    int a = 0, b = 1, i = 0, aux
    while (a + b) < n:
        if i > 0:
            show(",")
        end
        if i == 1:
            show("0")
        end
        if i == 1:
            show("1")
        end
        else:
            aux = a + b
            show(aux)
            a = b
            b = a + b
        end
    i = 1 + i
    end
end


